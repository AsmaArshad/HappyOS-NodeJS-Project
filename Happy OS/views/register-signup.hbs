<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/register-signup-style.css">
    <title>Register/Signup</title>
</head>

<body>
    <div class="register-signup">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    {{#if signup_error}}
                    <div class="alert alert-danger mt-3" role="alert">
                        {{signup_error}}
                    </div>
                    {{/if}}
                    <div class="row justify-content-center">
                        <div class="col-lg-4 col-xl-4 col-md-4 col-sm-8 col-10">
                            <div class="row justify-content-center">
                                <div class="col-4 text-center register-signup-img">
                                    <img src="/images/logo.png" class="img-fluid" alt="">
                                    <h4 class="mt-4 signup-heading">SIGN-UP</h4>
                                    <h4 class="mt-4 signup-heading">{{msg}}</h4>
                                </div>
                            </div>

                            <div
                                class="row justify-content-center align-content-center mt-2 bg-white pt-4 pb-4 white-cont">
                                <div class="col-lg-11">
                                    <div class="row">
                                        <form method="post">
                                            <div class="col-12 register-signup-card">
                                                <h4>Company</h4>
                                                <input type="text" placeholder="Company/Org name" name="org_name"
                                                    value="{{list.org_name}}" required>
                                                <input type="text" placeholder="Address" name="address"
                                                    value="{{list.address}}" required>

                                                <div class="row mt-3">

                                                    <div class="col-lg-2 col-xl-2 col-md-2 col-sm-2 col-2 drop-d">
                                                        <input class="city-row" type="text" placeholder="City"
                                                            name="city" value="{{list.city}}"
                                                            onkeydown="return (event.keyCode>=65 && event.keyCode<=90 || event.keyCode==32 || event.keyCode==8)"
                                                            required>
                                                    </div>

                                                    <div class="col-lg-2 col-xl-2 col-md-2 col-sm-2 col-2 drop-d"
                                                        required>
                                                        <input class="city-row" type="text" placeholder="State"
                                                            name="state" value="{{list.state}}"
                                                            onkeydown="return (event.keyCode>=65 && event.keyCode<=90 || event.keyCode==32 || event.keyCode==8)">
                                                    </div>

                                                    <div class="col-lg-2 col-xl-2 col-md-2 col-sm-2 col-2 drop-d"
                                                        required>
                                                        <input class="city-row" type="text" placeholder="Zipcode"
                                                            name="zipcode" value="{{list.zipcode}}"
                                                            onkeypress="return ((event.keyCode > 47 && event.keyCode < 58) || (event.keyCode == 46) || (event.keyCode == 45))"
                                                            required>
                                                    </div>
                                                </div>

                                                <div class="row justify-content-start align-items-center">
                                                    <div class="col-auto position-relative text-center">
                                                        <input class="phone-circle" type="text" placeholder="001"
                                                            name="country_code" value="{{list.country_code}}"
                                                            onkeypress="return ((event.keyCode > 47 && event.keyCode < 58) || (event.keyCode == 46) || (event.keyCode == 45))"
                                                            required>
                                                    </div>
                                                    <div class="col-6">
                                                        <input type="text" placeholder="Phone Number"
                                                            name="company_phone_number"
                                                            value="{{list.company_phone_number}}" maxlength="10"
                                                            pattern="\d{10}" title="Please enter exactly 10 digits"
                                                            onkeypress="return ((event.keyCode > 47 && event.keyCode < 58) || (event.keyCode == 46) || (event.keyCode == 45))"
                                                            required>
                                                    </div>
                                                </div>

                                                <div class="row mt-3 pt-3">
                                                    <h4>Contact</h4>
                                                </div>

                                                <div class="row">
                                                    <div class="col-6">
                                                        <input type="text" placeholder="First Name" name="first_name"
                                                            value="{{list.first_name}}" required>
                                                        <input type="text" placeholder="Title" name="title"
                                                            value="{{list.title}}" required>
                                                    </div>
                                                    <div class="col-6">
                                                        <input type="text" placeholder="Last Name" name="last_name"
                                                            value="{{list.last_name}}" required>
                                                        <div class="row justify-content-start align-items-center">
                                                            <div class="col-auto position-relative text-center">
                                                                <input class="phone-circle" type="text"
                                                                    placeholder="001" name="coun_code"
                                                                    value="{{list.coun_code}}"
                                                                    onkeypress="return ((event.keyCode > 47 && event.keyCode < 58) || (event.keyCode == 46) || (event.keyCode == 45))"
                                                                    required>
                                                            </div>

                                                            <div class="col-6">
                                                                <input class="ttt" type="text" placeholder="Phone"
                                                                    name="contact_phone_number"
                                                                    value="{{list.contact_phone_number}}" maxlength="10"
                                                                    pattern="\d{10}"
                                                                    title="Please enter exactly 10 digits"
                                                                    onkeypress="return ((event.keyCode > 47 && event.keyCode < 58) || (event.keyCode == 46) || (event.keyCode == 45))"
                                                                    required>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-12">
                                                        <input type="email" placeholder="Email Address"
                                                            value="{{list.Email}}" name="email" required>
                                                        <input type="text" placeholder="Confirm Email Address"
                                                            value="{{list.Email}}" name="confirm_email" required>
                                                        <input type="text" placeholder="User Name" name="user_name"
                                                            value="{{list.user_name}}" required>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-6">
                                                        <input type="password" placeholder="Password" name="password"
                                                            value="{{list.password}}" required>
                                                    </div>
                                                    <div class="col-6">
                                                        <input type="password" placeholder="Confirm Password"
                                                            name="confirm_password" value="{{list.confirm_password}}"
                                                            required>
                                                    </div>
                                                </div>


                                                <div class="row policy col-auto">
                                                    <div class="col-12">
                                                        <label>
                                                            <input type="checkbox" name="policy_check" required><span>I
                                                                agree with <a class="anchor">cookies</a> and <a
                                                                    class="anchor">privacy policies</a> </span>
                                                        </label>
                                                    </div>
                                                </div>

                                                <div class="row justify-content-center mt-4">
                                                    <div class="col-7 text-center">
                                                        <input type="submit" class="blue-btn" value="Get Started">
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
</body>

</html>